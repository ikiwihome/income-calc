{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "income-calc",
  "version": "1.0.0",
    "identifier": "cc.ikimi.income-calc",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "税后收入计算器",
        "width": 1400,
        "height": 800,
        "resizable": false,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "msi",
    "icon": [
      "../public/favicon.png",
      "../public/favicon.ico"
    ]
  }
}
